    <tool>stone sword</tool>
    <tool>bow</tool>
</toolrepair>
<timelock>true</timelock>
<hunger>
    <depletion>off</depletion>
</hunger>
<respawn delay="5s" auto="true"/>
<score>
	<kills>0</kills>
	<deaths>0</deaths>
  <limit>400</limit>
