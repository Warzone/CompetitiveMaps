<map proto="1.4.0">
<name>Mars</name>
<version>1.0.4</version>
<objective>Capture both of the enemy's flags.</objective>
<gamemode>ctf</gamemode>
<authors>
    <author uuid="9116d3fa-1400-4ada-aecd-2eae1d73e383"/> <!-- Xuph -->
    <author uuid="5198dc37-537f-4ea5-8ee8-1e0952d4a36b"/> <!-- 3each -->
</authors>
<contributors>
    <contributor uuid="0c44ca5a-6a42-49b7-81f2-58dc6d2ae3e9" contribution="XML Help"/> <!-- Xerocoles -->
    <contributor uuid="bbfac3db-dd46-4996-b936-0e2432f33285" contribution="XML Help"/> <!-- Valky -->
    <contributor uuid="2cef6d05-ec9a-44b3-bedc-53359c68ae65" contribution="XML Fix"/> <!-- BGM -->
</contributors>
<teams>
    <team id="red-team" color="dark red" max="7">Red</team>
    <team id="blue-team" color="blue" max="7">Blue</team>
</teams>
<spawns>
    <spawn team="red-team" kit="red-kit">
        <regions yaw="-90">
            <cuboid min="-67.5,18,-0.5" max="-65.5,18,1.5"/>
        </regions>
    </spawn>
    <spawn team="blue-team" kit="blue-kit">
        <regions yaw="90">
            <cuboid min="68.5,18,-0.5" max="66.5,18,1.5"/>
        </regions>
    </spawn>
    <default kit="obs-kit">
        <regions yaw="0">
            <cuboid min="2.5,59,-38.5" max="-1.5,59,-37.5"/>
        </regions>
    </default>
</spawns>
<respawn delay="9s"/>
<kits>
    <kit id="obs-kit">
        <potion duration="oo">night vision</potion>
    </kit>
    <kit id="spawn-kit">
        <clear/>
        <item slot="0" unbreakable="true">stone sword</item>
        <item slot="1" unbreakable="true">bow</item>
        <item slot="2" unbreakable="true">wood pickaxe</item>
        <item slot="4">golden apple</item>
        <item slot="5" amount="20">arrow</item>
        <leggings unbreakable="true" enchantment="protection projectile:2">chainmail leggings</leggings>
        <boots unbreakable="true" enchantment="protection fall:1">iron boots</boots>
        <potion duration="oo">night vision</potion>
    </kit>
    <kit id="blue-kit" parents="spawn-kit">
        <item slot="3" damage="11" amount="8">stained clay</item>
        <helmet color="334CB2" unbreakable="true">leather helmet</helmet>
        <chestplate color="334CB2" unbreakable="true">leather chestplate</chestplate>
    </kit>
    <kit id="red-kit" parents="spawn-kit">
        <item slot="3" damage="14" amount="8">stained clay</item>
        <helmet color="993333" unbreakable="true">leather helmet</helmet>
        <chestplate color="993333" unbreakable="true">leather chestplate</chestplate>
    </kit>
    <kit id="flag-kit">
        <potion duration="oo">slowness</potion>
        <potion duration="oo">jump</potion>
    </kit>
    <kit id="reset-kit" force="true">
        <potion duration="0" amplifier="10">slowness</potion>
        <potion duration="0" amplifier="10">jump</potion>
    </kit>
</kits>
<filters>
    <team id="red-only">red-team</team>
    <team id="blue-only">blue-team</team>
    <time id="flag-respawns">0</time>
    <any id="only-clay">
        <all>
            <material>stained clay</material>
        </all>
    </any>
</filters>
<regions>
    <cuboid id="anti-under" min="oo,9,oo" max="-oo,0,-oo"/>
    <apply block="never" region="anti-under"/>
    <union id="clay-lanes">
        <rectangle min="-24,29" max="-46,36"/>
        <rectangle min="-24,-28" max="-46,-35"/>
        <rectangle min="25,36" max="47,29"/>
        <rectangle min="47,-28" max="25,-35"/>
    </union>
    <apply block="only-clay" region="clay-lanes"/>
    <apply block="never" region="everywhere"/>
    <cylinder id="blue-net" base="67.5,18,0.5" radius="2" height="2" />
    <cylinder id="red-net" base="-66.5,18,0.5" radius="2" height="2" />
</regions>
<flags flag-proximity-metric="closest player">
    <flag id="north-red-flag" name="North Red Flag" owner="red-team" pickup-kit="flag-kit" drop-kit="reset-kit">
        <post pickup-filter="blue-only" recover-time="15s" respawn-time="5s">-61.5,25,-27.5</post>
    </flag>
    <flag id="south-red-flag" name="South Red Flag" owner="red-team" pickup-kit="flag-kit" drop-kit="reset-kit">
        <post pickup-filter="blue-only" recover-time="15s" respawn-time="5s">-61.5,25,28.5</post>
    </flag>
    <flag id="south-blue-flag" name="South Blue Flag" owner="blue-team" pickup-kit="flag-kit" drop-kit="reset-kit">>
        <post pickup-filter="red-only" recover-time="15s" respawn-time="5s">62.5,25,28.5</post>
    </flag>
    <flag id="north-blue-flag" name="North Blue Flag" owner="blue-team" pickup-kit="flag-kit" drop-kit="reset-kit">>
        <post pickup-filter="red-only" recover-time="15s" respawn-time="5s">62.5,25,-27.5</post>
    </flag>
    <flags points="1">
        <net region="red-net" flag="north-blue-flag south-blue-flag" respawn-filter="flag-respawns"/>
        <net region="blue-net" flag="north-red-flag south-red-flag" respawn-filter="flag-respawns"/>
    </flags>
</flags>
<time>12m</time>
<score/>
<killreward>
    <item amount="8">arrow</item>
    <item>golden apple</item>
</killreward>
<killreward>
    <filter>
        <all>
            <team>red-team</team>
        </all>
    </filter>
    <item slot="3" damage="14" amount="8">stained clay</item>
</killreward>
<killreward>
    <filter>
        <all>
            <team>blue-team</team>
        </all>
    </filter>
    <item slot="3" damage="11" amount="8">stained clay</item>
</killreward>
<itemremove>
    <item>leather helmet</item>
    <item>leather chestplate</item>
    <item>chainmail leggings</item>
    <item>iron boots</item>
</itemremove>
<itemkeep>
    <item>golden apple</item>
    <item>arrow</item>
    <item>wood pickaxe</item>
    <item>stained clay</item>
</itemkeep>
<toolrepair>
    <tool>stone sword</tool>
    <tool>bow</tool>
</toolrepair>
<hunger>
    <depletion>off</depletion>
</hunger>
<timelock>on</timelock>
<maxbuildheight>28</maxbuildheight>
</map>
